POST	/historiaClinicaDetalle 				controllers.restServices.HistoriaClinicaDetalleController.nuevo()
GET		/historiaClinicaDetalle/borrar/:id		controllers.restServices.HistoriaClinicaDetalleController.delete(id: Long)


#Odontologo
