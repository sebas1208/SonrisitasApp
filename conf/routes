# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET		/login 						controllers.Login.login()
GET		/prueba						controllers.Prueba.prueba()
GET 	/odontologo					controllers.OdontologoController.all()
GET 	/odontologo/:id				controllers.OdontologoController.one(id)
#POST 	/odontologo 				controllers.OdontologoController.submitOdontologo()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
