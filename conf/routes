GET 	/odontologo/:id			controllers.restServices.OdontologoController.one(id: Long)
POST	/odontologo 			controllers.restServices.OdontologoController.nuevo()
GET		/odontologo/borrar/:id	controllers.restServices.OdontologoController.delete(id: Long)

