# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~


#Administrador
GET 	/administradores				controllers.restServices.AdministradorController.all()
GET 	/administrador/:id			controllers.restServices.AdministradorController.one(id: Long)
POST	/administrador 				controllers.restServices.AdministradorController.nuevo()
GET		/administrador/borrar/:id	controllers.restServices.AdministradorController.delete(id: Long)

#Agenda Odontologo
GET 	/agendasOdontologo			controllers.restServices.AgendaOdontologoController.all()
GET 	/agendaOdontologo/:id		controllers.restServices.AgendaOdontologoController.one(id: Long)
POST	/agendaOdontologo 			controllers.restServices.AgendaOdontologoController.nuevo()
GET		/agendaOdontologo/borrar/:id controllers.restServices.AgendaOdontologoController.delete(id: Long)

#Atencion Medica
GET 	/atencionesMedicas				controllers.restServices.AtencionMedicaController.all()
GET 	/atencionMedica/:id			controllers.restServices.AtencionMedicaController.one(id: Long)
POST	/atencionMedica 			controllers.restServices.AtencionMedicaController.nuevo()
GET		/atencionMedica/borrar/:id	controllers.restServices.AtencionMedicaController.delete(id: Long)

#Especialidad
GET 	/especilidades				controllers.restServices.EspecialidadController.all()
GET 	/especialidad/:id			controllers.restServices.EspecialidadController.one(id: Long)
POST	/especialidad 				controllers.restServices.EspecialidadController.nuevo()
GET		/especialidad/borrar/:id	controllers.restServices.EspecialidadController.delete(id: Long)

#Historia Clinica Cabecera
