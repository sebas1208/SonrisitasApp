GET 	/historiaClinicaCabecera/:id			controllers.restServices.HistoriaClinicaCabeceraController.one(id: Long)
POST	/historiaClinicaCabecera 				controllers.restServices.HistoriaClinicaCabeceraController.nuevo()
GET		/historiaClinicaCabecera/borrar/:id		controllers.restServices.HistoriaClinicaCabeceraController.delete(id: Long)


#Historia Clinica Detalle
